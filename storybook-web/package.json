{
  "name": "storybook-web",
  "version": "1.0.0",
  "description": "Web interface for Storybook manuscript editor",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev",
    "dev:server": "tsx watch server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "concurrently \"next start\" \"node dist/server/index.js\"",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.4",
    "next": "14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "axios": "^1.7.9",
    "zustand": "^5.0.2",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "date-fns": "^4.1.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "formidable": "^3.5.2",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/formidable": "^3.4.5",
    "@types/uuid": "^10.0.0",
    "typescript": "^5",
    "tsx": "^4.19.2",
    "tailwindcss": "^3.4.1",
    "postcss": "^8",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "concurrently": "^9.1.0"
  }
}
